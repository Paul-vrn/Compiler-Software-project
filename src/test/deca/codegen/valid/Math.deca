class Math{

    int fib(int n)
    {
        if (n <= 1){
            return n;}
        return fib(n - 1) + fib(n - 2);
    }


    boolean isEven(int n){
	if(n%2==0){return true;}
	else{return false;}
	}
	

	boolean isNotEven(int n){
		return !isEven(n);
	}

	int fact(int n){
		if(n==0 || n==1){
			return 1;
		}
		else{
			return n*fact(n-1);
	}
}
}




 {
 Math math = new Math();
 int i = 1;
 int res;
 while(i<10){
    res = math.fib(i);
    println(res);
    i = i + 1;
   }

 i = 1;
 while(i<10){
	res = math.fact(i);
	println(res);
	i = i+1;
  }
}

